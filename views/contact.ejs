<%- include('layouts/head')  %>
<%- include('layouts/navbar') %> 

<div class="container">
 
  <div class="row">
    <div class="col-md-8">
      <h1>Daftar kontak</h1>
      <table class="table">
        <thead>
          <tr>
            <th scope="col">No</th>
            <th scope="col">Nama</th>
            <th scope="col">Email</th>
            <th scope="col">Aksi</th>
          </tr>
        </thead>
        <tbody>
          <% allDataContact.forEach((contact, i) => { %>
            <tr>
              <th scope="row"><%= i + 1 %> </th>
              <td><%= contact.name %> </td>
              <td><%= contact.email %>  </td>
              <td>
                <a href="/contact/<%= contact.name %> " class="btn btn-primary badge rounded">
                  Detail
                </a>
              </td>
            </tr>
          <% }) %>
         
        </tbody>
      </table>
    </div>
  </div>
</div>

<%- include('layouts/footer') %>